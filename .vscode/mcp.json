{
  "mcpServers": {
    "context7": {
      "command": "npx",
      "args": ["-y", "@upstash/context7-mcp@latest"]
    },
    "serena": {
      "command": "uvx",
      "args": [
        "--from",
        "git+https://github.com/oraios/serena",
        "serena",
        "start-mcp-server",
        "--context",
        "ide-assistant",
        "--project",
        ".",
        "--enable-gui-log-window",
        "false",
        "--enable-web-dashboard",
        "false"
      ]
    },
    "kiri": {
      "command": "npx",
      "args": [
        "kiri-mcp-server@latest",
        "--repo",
        ".",
        "--db",
        ".kiri/index.duckdb",
        "--watch"
      ]
    },
    "chrome-devtools": {
      "command": "npx",
      "args": ["chrome-devtools-mcp@latest"]
    },
    "next-devtools": {
      "command": "npx",
      "args": ["-y", "next-devtools-mcp@latest"]
    }
  }
}
